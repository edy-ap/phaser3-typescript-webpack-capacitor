{
  "name": "phaser3-typescript",
  "version": "0.0.1",
  "description": "Boilerplate for Phaser 3 in Typescript",
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "TypeScript",
    "Webpack"
  ],
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "expose-loader": "0.7.5",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "5.1.1",
    "typescript": "3.0.3",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@capacitor/android": "^1.0.0-beta.8",
    "@capacitor/cli": "^1.0.0-beta.8",
    "@capacitor/core": "^1.0.0-beta.8",
    "phaser": "3.15.1"
  },
  "scripts": {
    "serve": "webpack --config webpack.dev.config.js --progress && webpack-dev-server --config webpack.dev.config.js",
    "serve:prod": "webpack --progress && webpack-dev-server",
    "build": "webpack --progress && npx cap copy"
